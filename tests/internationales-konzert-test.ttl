@prefix : <http://example.org/kulturarena/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://example.org/kulturarena/> .

<http://example.org/kulturarena/> rdf:type owl:Ontology ;
                                   rdfs:label "Kulturarena Ontologie"@de .

#################################################################
#    Object Properties
#################################################################

###  http://example.org/kulturarena/hatMusikdarbieter
:hatMusikdarbieter rdf:type owl:ObjectProperty ;
                   rdfs:domain :Konzert ;
                   rdfs:range :Musikdarbieter ;
                   rdfs:comment "gibt den Musikdarbieter eines Konzerts an"@de ;
                   rdfs:label "hatMusikdarbieter"@de .


###  http://example.org/kulturarena/kommtAus
:kommtAus rdf:type owl:ObjectProperty ;
          rdfs:domain :Musikdarbieter ;
          rdfs:range :Land ;
          rdfs:comment "gibt das Herkunftsland einer Entität an"@de ;
          rdfs:label "kommtAus"@de .


#################################################################
#    Data properties
#################################################################

###  http://example.org/kulturarena/Einwohnerzahl
:Einwohnerzahl rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :Land .


###  http://example.org/kulturarena/hatName
:hatName rdf:type owl:DatatypeProperty ;
         rdfs:domain owl:Thing ;
         rdfs:comment "gibt Name einer Entität an"@de ;
         rdfs:label "hatName"@de .


#################################################################
#    Classes
#################################################################

###  http://example.org/kulturarena/Ausland
:Ausland rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( :Land
                                                    [ rdf:type owl:Class ;
                                                      owl:complementOf [ rdf:type owl:Class ;
                                                                         owl:oneOf ( :Deutschland
                                                                                   )
                                                                       ]
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:subClassOf :Land .


###  http://example.org/kulturarena/Konzert
:Konzert rdf:type owl:Class ;
         owl:disjointWith :Land ,
                          :Musikdarbieter ;
         rdfs:comment "Auffuehrung eines Musikwerks"@de ;
         rdfs:label "Konzert"@de .


###  http://example.org/kulturarena/Land
:Land rdf:type owl:Class ;
      owl:disjointWith :Musikdarbieter ;
      rdfs:comment "politisch selbststaendiges, von Grenzen umgebenes Gebiet"@de ;
      rdfs:label "Land"@de .


###  http://example.org/kulturarena/Musikdarbieter
:Musikdarbieter rdf:type owl:Class ;
                rdfs:comment "Musikdarbieter sind Solisten oder Musikensembles"@de ;
                rdfs:label "Musikdarbieter"@de .


###  http://example.org/kulturarena/internationalesKonzert
:internationalesKonzert rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Restriction ;
                                              owl:onProperty :hatMusikdarbieter ;
                                              owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :kommtAus ;
                                                                   owl:someValuesFrom [ rdf:type owl:Class ;
                                                                                        owl:complementOf [ rdf:type owl:Class ;
                                                                                                           owl:oneOf ( :Deutschland
                                                                                                                     )
                                                                                                         ]
                                                                                      ]
                                                                 ]
                                            ] ;
                        rdfs:subClassOf :Konzert ;
                        rdfs:comment "Konzert mit Musikdarbietern, die nicht aus Deutschland kommen"@de ;
                        rdfs:label "internationalesKonzert"@de .


#################################################################
#    Individuals
#################################################################

###  http://example.org/kulturarena/AlJarreau
:AlJarreau rdf:type owl:NamedIndividual ,
                    :Musikdarbieter ;
           :kommtAus :USA ;
           :hatName "Al Jarreau"@de .


###  http://example.org/kulturarena/AlJarreauKonzert2005
:AlJarreauKonzert2005 rdf:type owl:NamedIndividual ,
                               :Konzert ;
                      :hatMusikdarbieter :AlJarreau .


###  http://example.org/kulturarena/Deutschland
:Deutschland rdf:type owl:NamedIndividual ,
                      :Land ;
             :Einwohnerzahl 456 ;
             :hatName "Deutschland"@de ;
             rdfs:label "Deutschland"@de .


###  http://example.org/kulturarena/USA
:USA rdf:type owl:NamedIndividual ,
              :Land ;
     :hatName "USA"@de ;
     rdfs:label "USA"@de .


