###  http://example.org/kulturarena/Musikdarbieter
:Musikdarbieter rdf:type owl:Class ;
                rdfs:subClassOf owl:Thing ;
                owl:disjointWith owl:Nothing ;
                rdfs:comment "Musikdarbieter sind Solisten oder Musikensembles"@de ;
                rdfs:label "Musikdarbieter"@de .


###  http://example.org/kulturarena/Musikensemble
:Musikensemble rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( :Musikdarbieter
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :wikidataType ;
                                                            owl:someValuesFrom [ owl:intersectionOf ( owl:Thing
                                                                                                      [ rdf:type owl:Class ;
                                                                                                        owl:complementOf [ rdf:type owl:Class ;
                                                                                                                           owl:oneOf ( <http://www.wikidata.org/entity/Q5>
                                                                                                                                     )
                                                                                                                         ]
                                                                                                      ]
                                                                                                    ) ;
                                                                                 rdf:type owl:Class
                                                                               ]
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ,
                                   [ owl:intersectionOf ( :Musikdarbieter
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :hatMitglied ;
                                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass :Musiker
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:subClassOf :Musikdarbieter ;
               owl:disjointWith :Solist ,
                                owl:Nothing ;
               rdfs:comment "Musikensembles bestehen aus mehreren Musikern"@de ;
               rdfs:label "Musikensemble"@de .


###  http://example.org/kulturarena/Musiker
:Musiker rdf:type owl:Class ;
         rdfs:subClassOf owl:Thing ;
         owl:disjointWith owl:Nothing ;
         rdfs:comment "Person, die Musik spielt"@de ;
         rdfs:label "Musiker"@de .


###  http://example.org/kulturarena/Solist
:Solist rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( :Musikdarbieter
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :wikidataType ;
                                                     owl:hasValue <http://www.wikidata.org/entity/Q5>
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf :Musikdarbieter ;
        owl:disjointWith owl:Nothing ;
        rdfs:comment "Solisten sind Musiker, die alleine auftreten."@de ;
        rdfs:label "Solist"@de .


###  http://example.org/kulturarena/internationalesKonzert
:internationalesKonzert rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( :Konzert
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty :hatMusikdarbieter ;
                                                                     owl:someValuesFrom [ owl:intersectionOf ( :Musikdarbieter
                                                                                                               [ rdf:type owl:Restriction ;
                                                                                                                 owl:onProperty :kommtAus ;
                                                                                                                 owl:someValuesFrom [ owl:intersectionOf ( :Land
                                                                                                                                                           [ rdf:type owl:Class ;
                                                                                                                                                             owl:complementOf [ rdf:type owl:Class ;
                                                                                                                                                                                owl:oneOf ( :Deutschland
                                                                                                                                                                                          )
                                                                                                                                                                              ]
                                                                                                                                                           ]
                                                                                                                                                         ) ;
                                                                                                                                      rdf:type owl:Class
                                                                                                                                    ]
                                                                                                               ]
                                                                                                             ) ;
                                                                                          rdf:type owl:Class
                                                                                        ]
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:subClassOf :Konzert ;
