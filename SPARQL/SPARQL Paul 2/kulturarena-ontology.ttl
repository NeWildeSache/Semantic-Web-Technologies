#################################################################
#    Classes
#################################################################

###  http://example.org/kulturarena/Ereignis
ex:Ereignis rdf:type owl:Class ;
            rdfs:subClassOf ex:Veranstaltungsort ;
            owl:disjointWith ex:Genre ,
                             ex:Geschlecht ,
                             ex:Künstler ,
                             ex:Land ;
            rdfs:comment "Ereignis, hier kongruent zu Veranstaltung und Konzert." ;
            rdfs:label "Ereignis" .


###  http://example.org/kulturarena/Genre
ex:Genre rdf:type owl:Class ;
         owl:disjointWith ex:Geschlecht ,
                          ex:Künstler ,
                          ex:Land ,
                          ex:Veranstaltung ,
                          ex:Veranstaltungsort ;
         rdfs:comment "Genre mit dem die Künstler/in assoziiert wird." ;
         rdfs:label "Genre" .


###  http://example.org/kulturarena/Geschlecht
ex:Geschlecht rdf:type owl:Class ;
              owl:disjointWith ex:Künstler ,
                               ex:Land ,
                               ex:Veranstaltung ,
                               ex:Veranstaltungsort ;
              rdfs:comment "Geschlecht der Künstler/in." ;
              rdfs:label "Geschlecht" .


###  http://example.org/kulturarena/Konzert
ex:Konzert rdf:type owl:Class ;
           rdfs:subClassOf ex:Veranstaltung ;
           rdfs:comment "Konzert, hierbei kongruent zu Ereignis und Veranstaltung." ;
           rdfs:label "Konzert" .


###  http://example.org/kulturarena/Künstler
ex:Künstler rdf:type owl:Class ;
            owl:disjointWith ex:Land ,
                             ex:Veranstaltung ,
                             ex:Veranstaltungsort ;
            rdfs:comment "Einzelner Künstler oder Gruppe." ;
            rdfs:label "Künstler" .


###  http://example.org/kulturarena/Land
ex:Land rdf:type owl:Class ;
        owl:disjointWith ex:Veranstaltung ,
                         ex:Veranstaltungsort ;
        rdfs:comment "Herkunftsland der Künstler/innen." ;
        rdfs:label "Land" .


###  http://example.org/kulturarena/Veranstaltung
ex:Veranstaltung rdf:type owl:Class ;
                 rdfs:subClassOf ex:Ereignis ;
                 rdfs:comment "Veranstaltung, hierbei kongruent zu Konzert und Ereignis." ;
                 rdfs:label "Veranstaltung" .


###  http://example.org/kulturarena/Veranstaltungsort
ex:Veranstaltungsort rdf:type owl:Class ;
                     rdfs:comment "Veranstaltungsort, hierbei immer Kulturarena." ;
                     rdfs:label "Veranstaltungsort" .


