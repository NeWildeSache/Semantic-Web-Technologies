# CQ8: Welche Musikdarbieter stammen aus dem bev√∂lkerungsreichsten vertretenen Land?
# (Musikdarbietern-IRI, Musikdarbietern-Name, Herkunftsland-Name)

SELECT DISTINCT ?darbieter ?name ?land
WHERE {
  {
  SELECT ?land (MAX(?einwohnerzahl) AS ?maxEinwohnerzahl)
  WHERE {
    ?land :hatEinwohneranzahl ?einwohnerzahl
  }
  GROUP BY ?land
  ORDER BY DESC (?maxEinwohnerzahl)
  LIMIT 1
  }
  ?konzert :hatMusikdarbieter ?darbieter .
  ?darbieter :kommtAus ?land ;
             :hatName ?name .
} 
