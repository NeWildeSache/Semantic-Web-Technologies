@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix :     <http://example.org/kulturarena/> .


<http://example.org/kulturarena/> a owl:Ontology ;
    rdfs:label "Kulturarena Ontologie"@de .




# CLASSES
:Konzert a owl:Class ;
        rdfs:label "Konzert"@de ;
        rdfs:comment "Auffuehrung eines Musikwerks"@de.


:Musikensemble a owl:Class ;
        rdfs:label "Musikensemble"@de ;
        rdfs:comment "Musikensembles bestehen aus mehreren EnsembleMitgliedern"@de .


:Solist a owl:Class ;
        rdfs:label "Solist"@de ;
        rdfs:comment "Solisten sind Musiker, die alleine auftreten."@de .


:Land a owl:Class ;
        rdfs:label "Land"@de ;
        rdfs:comment "politisch selbststaendiges, von Grenzen umgebenes Gebiet"@de .


:Frau a owl:Class ;
        rdfs:label "Frau"@de ;
        rdfs:comment "weibliche Person"@de .


:Mann a owl:Class ;
        rdfs:label "Mann"@de ;
        rdfs:comment "maennliche Person"@de .


:Genre a owl:Class ;
        rdfs:label "Genre"@de ;
        rdfs:comment "Musikgenre"@de .


:DiversePerson a owl:Class ;
        rdfs:label "DiversePerson"@de ;
        rdfs:comment "Person, die sich weder als männlich noch weiblich identifiziert"@de .


:EnsembleMitglied a owl:Class ;
        rdfs:label "EnsembleMitglied"@de ;
        rdfs:comment "Musiker, welcher in einem Musikensemble mitwirkt"@de .




# PROPERTIES
:istMitgliedIn a owl:ObjectProperty ;
rdfs:label "istMitgliedIn"@de ;
        rdfs:comment "gibt an, welchem Musikensemble ein EnsembleMitglied angehört"@de ;
        rdfs:domain :EnsembleMitglied ;
        rdfs:range :Musikensemble  .


:hatMitglied a owl:ObjectProperty ;
        rdfs:label "istMitgliedIn"@de ;
        rdfs:comment "gibt an, welche EnsembleMitglieder dem Musikensemble angehören"@de ;
        rdfs:domain :Musikensemble  ;
        rdfs:range :EnsembleMitglied .


:hatEinwohneranzahl a owl:DatatypeProperty ;
        rdfs:label "hatEinwohneranzahl"@de ;
        rdfs:comment "gibt an, wie viele Einwohner ein Land hat"@de ;
        rdfs:domain :Land ;
        rdfs:range xsd:Integer .


:hatGeschlecht a owl:ObjectProperty ;
        rdfs:label "hatGeschlecht"@de ;
        rdfs:comment "gibt das Geschlecht eines Solisten oder eines EnsembleMitglieds an"@de ;
rdfs:domain [ a owl:Class ;
owl:unionOf (:Solist :EnsembleMitglied) ] ;
        rdfs:range [ a owl:Class ;
                       owl:unionOf (:Mann :Frau :DiversePerson) ] .


:hatName a owl:DatatypeProperty ;
        rdfs:label "hatName"@de ;
        rdfs:comment "gibt Name einer Entität an"@de ;
        rdfs:range xsd:string .


:fandStattImJahr a owl:DatatypeProperty ;
        rdfs:label "fandStattImJahr"@de ;
        rdfs:comment "gibt an, in welchem Jahr ein Konzert stattfand"@de ;
        rdfs:domain :Konzert ;
        rdfs:range xsd:gYear .


:hatMusikdarbieter a owl:ObjectProperty ;
        rdfs:label "hatMusikdarbieter"@de ;
        rdfs:comment "gibt den Musikdarbieter eines Konzerts an"@de ;
        rdfs:domain :Konzert ;
        rdfs:range [ a owl:Class ;
                                owl:unionOf (:Solist :Musikensemble) ] .


:kommtAus a owl:ObjectProperty ;
        rdfs:label "kommtAus"@de ;
        rdfs:comment "gibt das Herkunftsland eines Solisten oder Musikensembles an"@de ;
        rdfs:domain [ a owl:Class ;
                                owl:unionOf (:Solist :Musikensemble) ] ;
        rdfs:range :Land .


:hatGenre a owl:ObjectProperty ; 
        rdfs:label "hatGenre"@de ;
        rdfs:comment "gibt das Genre eines Musikdarbieters an"@de ;
        rdfs:domain [ a owl:Class ;
                                owl:unionOf (:Solist :Musikensemble) ] ;
        rdfs:range :Genre .


:hatMusicBrainzArtistID a owl:DatatypeProperty ;
        rdfs:label "hatMusicBrainzArtistID"@de ;
        rdfs:comment "gibt die MusicBrainz-ArtistID eines Solisten oder Musikensembles an"@de ;
        rdfs:domain [ a owl:Class ;
                                owl:unionOf (:Solist :Musikensemble) ] ;
        rdfs:range xsd:string .


:hatGeburtsdatum a owl:DatatypeProperty ;
        rdfs:label "hatGeburtsjahr"@de ;
        rdfs:comment "gibt das Geburtsdatum eines Solisten oder eines EnsembleMitglieds an"@de ;
        rdfs:domain [ a owl:Class ;
                                owl:unionOf (:Solist :EnsembleMitglied) ] ;
        rdfs:range xsd:date .












# DISJOINTNESS
[ rdf:type owl:AllDisjointClasses ; 
owl:members ( 
:Frau 
:Mann 
:DiversePerson 
:Land
        :Genre
        :Konzert
        :Musikensemble


) ] .


:Solist owl:disjointWith :Musikensemble .
:Solist owl:disjointWith :EnsembleMitglied .












