PREFIX rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl:  <http://www.w3.org/2002/07/owl#>
PREFIX xsd:  <http://www.w3.org/2001/XMLSchema#>
PREFIX :     <http://example.org/kulturarena/>
# CQ1: Welche Musikdarbieter (Solist*innen oder Ensembles) 
#      traten im Jahr 2015 auf?
#      (Musikdarbietern-IRI, Musikdarbietern-Name)
SELECT ?mdIRI ?mdName WHERE {
  ?mdIRI a/rdfs:subClassOf* :Musikdarbieter ;
    rdfs:label ?mdName .
  ?konzert a :Konzert ;
    :hatMusikdarbieter ?mdIRI ;
    :hatDatum "2015"^^xsd:gYear .
}